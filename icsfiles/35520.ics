<script>
            
            jQuery( document ).ready( function( $ ) {

                function fadeOutCookieBanner( callback ) {

                    $( '.cookieBannerContainer' ).fadeOut( 1000, function() { callback } );

                }

                function setCookieBannerCookie() {
                
                    $.cookie( 'cookie_banner', false, { expires: 31, path: '/' } ); 
                    
                }

                $( '.cookieBannerRemove, .cookieBannerRemoveResponsive' ).on( 'click', function() {

                    fadeOutCookieBanner( setCookieBannerCookie() );
                    
                });

                

            });
            
        </script><style>
            
            .cookieBannerContainer
            {
                padding: 0px 5%;
                position: fixed;
                top: 0px;
                height: 24px;
                line-height: 24px;
                background: #636363;
                color: #ffffff;
                z-index: 9999;
                font-size: 11px;
                opacity: 0.8;
                text-align: right;
                left: 0px;
                right: 0px;
            }

            .cookieBannerText
            {
                text-align: left;
                display: inline-block;
                float: left;
            }

            .cookieBannerText a,
            .cookieBannerText a:link,
            .cookieBannerText a:visited,
            .cookieBannerText a:hover,
            .cookieBannerText a:active,
            .cookieBannerText a:focus
            {
                color: #ffffff;
            }  

            .cookieBannerRemove
            {
                display: inline-block;
                float: right;
                border: 1px solid #3f3f3f;
                height: 15px;
                line-height: 15px;
                background: #727272;
                padding: 1px 5px;
                cursor: pointer;
                color: #ffffff;
                margin-top: 2px;
            }

            .cookieBannerRemove:hover
            {
                background: #939393;
                color: #ffffff;
                border-color: #ffffff;
            }

            .cookieBannerRemoveContainer
            {
                line-height: 24px;
                height: 24px;
            }

            .cookieBannerRemoveResponsive
            {
                display: none;
                float: right;     
                cursor: pointer;
                color: #ffffff;
                position: absolute;
                top: 0px;
                right: 5%;
            }

            @media only screen and (min-width : 0px) and ( max-width : 700px ) { 

                .cookieBannerRemove
                {
                    display: none;
                }

                .cookieBannerRemoveResponsive
                {
                    display: inline-block;
                    right: 2%;
                }

                .cookieBannerContainer
                {
                    padding: 0px 2%;
                }

                .cookieBannerText
                {
                    font-size: 9px;
                    width: 95%;
                }

            }</style>
